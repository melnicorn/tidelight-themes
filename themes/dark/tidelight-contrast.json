{
  "name": "Tidelight Contrast",
  "type": "dark",
  "colors": {
    "focusBorder": "#22d3ee",

    "foreground": "#f8fafc",
    "descriptionForeground": "#cbd5e1",
    "errorForeground": "#fb7185",

    "editor.background": "#05070c",
    "editor.foreground": "#f8fafc",

    "editorLineNumber.foreground": "#334155",
    "editorLineNumber.activeForeground": "#e2e8f0",

    "editorCursor.foreground": "#22d3ee",
    "editor.selectionBackground": "#22d3ee2e",
    "editor.inactiveSelectionBackground": "#22d3ee20",
    "editor.wordHighlightBackground": "#38bdf830",
    "editor.wordHighlightStrongBackground": "#38bdf84a",
    "editor.findMatchBackground": "#fbbf244d",
    "editor.findMatchHighlightBackground": "#fbbf2433",
    "editor.hoverHighlightBackground": "#22d3ee1a",
    "editor.lineHighlightBackground": "#0b1220cc",

    "editorIndentGuide.background1": "#1f2937",
    "editorIndentGuide.activeBackground1": "#94a3b8",

    "editorBracketMatch.background": "#22d3ee26",
    "editorBracketMatch.border": "#22d3ee99",

    "editorGroupHeader.tabsBackground": "#05070c",
    "tab.activeBackground": "#0b1220",
    "tab.inactiveBackground": "#05070c",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveForeground": "#cbd5e1",
    "tab.border": "#0b1220",

    "activityBar.background": "#04060a",
    "activityBar.foreground": "#f8fafc",
    "activityBar.inactiveForeground": "#94a3b8",
    "activityBar.border": "#0b1220",

    "sideBar.background": "#04060a",
    "sideBar.foreground": "#f8fafc",
    "sideBar.border": "#0b1220",
    "sideBarTitle.foreground": "#ffffff",

    "list.activeSelectionBackground": "#22d3ee2e",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#0b1220",
    "list.hoverForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#0b1220",

    "statusBar.background": "#04060a",
    "statusBar.foreground": "#f8fafc",
    "statusBar.border": "#0b1220",
    "statusBar.debuggingBackground": "#1d4ed8",
    "statusBar.debuggingForeground": "#ffffff",

    "titleBar.activeBackground": "#04060a",
    "titleBar.activeForeground": "#f8fafc",
    "titleBar.inactiveBackground": "#04060a",
    "titleBar.inactiveForeground": "#94a3b8",

    "panel.background": "#05070c",
    "panel.border": "#0b1220",

    "terminal.background": "#05070c",
    "terminal.foreground": "#f8fafc",
    "terminalCursor.foreground": "#22d3ee",
    "terminal.ansiBlack": "#0b1220",
    "terminal.ansiBlue": "#60a5fa",
    "terminal.ansiCyan": "#22d3ee",
    "terminal.ansiGreen": "#a3e635",
    "terminal.ansiMagenta": "#f472b6",
    "terminal.ansiRed": "#fb7185",
    "terminal.ansiYellow": "#fbbf24",
    "terminal.ansiWhite": "#ffffff",

    "input.background": "#0b1220",
    "input.foreground": "#ffffff",
    "input.placeholderForeground": "#94a3b8",
    "input.border": "#1f2937",

    "dropdown.background": "#0b1220",
    "dropdown.foreground": "#ffffff",
    "dropdown.border": "#1f2937",

    "badge.background": "#22d3ee",
    "badge.foreground": "#06242b",

    "notificationCenterHeader.background": "#04060a",
    "notifications.background": "#0b1220",
    "notifications.foreground": "#ffffff",
    "notifications.border": "#1f2937",

    "editorOverviewRuler.errorForeground": "#fb7185ee",
    "editorOverviewRuler.warningForeground": "#fbbf24ee",
    "editorOverviewRuler.infoForeground": "#60a5faee",

    "gitDecoration.addedResourceForeground": "#a3e635",
    "gitDecoration.modifiedResourceForeground": "#60a5fa",
    "gitDecoration.deletedResourceForeground": "#fb7185",
    "gitDecoration.untrackedResourceForeground": "#22d3ee",
    "gitDecoration.ignoredResourceForeground": "#94a3b8"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#94a3b8", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "Template strings / interpolation",
      "scope": [
        "string.template",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "meta.template.expression"
      ],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#fbbf24" }
    },
    {
      "name": "Language constants (true/false/null/etc.)",
      "scope": ["constant.language"],
      "settings": { "foreground": "#f472b6" }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Operators / punctuation",
      "scope": ["keyword.operator", "punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#e2e8f0" }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "meta.method-call"
      ],
      "settings": { "foreground": "#60a5fa" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.parameters", "meta.function.parameters"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable", "entity.name.variable"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "name": "Constants",
      "scope": ["variable.other.constant", "entity.name.constant"],
      "settings": { "foreground": "#fbbf24" }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.struct",
        "entity.name.enum"
      ],
      "settings": { "foreground": "#93c5fd" }
    },
    {
      "name": "Type parameters / generics",
      "scope": ["entity.name.type.parameter", "meta.type.parameters", "meta.type.annotation"],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Language keywords for modules (import/export/etc.)",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from", "keyword.control.as"],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Self references (this/self/super)",
      "scope": ["variable.language.this", "variable.language.super", "variable.language.self"],
      "settings": { "foreground": "#f472b6" }
    },
    {
      "name": "Property names",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": { "foreground": "#c7d2fe" }
    },
    {
      "name": "Tags (HTML/XML/JSX)",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Attributes (HTML/XML/JSX)",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#93c5fd" }
    },
    {
      "name": "Invalid / deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": { "foreground": "#fb7185", "fontStyle": "underline" }
    },

    {
      "name": "JSON keys (Tidelight Contrast)",
      "scope": [
        "support.type.property-name.json",
        "meta.object-literal.key.json string.quoted.double.json",
        "meta.object.member.json string.quoted.double.json"
      ],
      "settings": { "foreground": "#c7d2fe" }
    },
    {
      "name": "JSON string values (Tidelight Contrast)",
      "scope": [
        "meta.object-literal.value.json string.quoted.double.json",
        "meta.array.literal.json string.quoted.double.json",
        "string.quoted.double.json"
      ],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "JSON numbers (Tidelight Contrast)",
      "scope": ["constant.numeric.json"],
      "settings": { "foreground": "#fbbf24" }
    },
    {
      "name": "JSON booleans & null (Tidelight Contrast)",
      "scope": [
        "constant.language.boolean.json",
        "constant.language.null.json",
        "constant.language.json"
      ],
      "settings": { "foreground": "#f472b6" }
    },
    {
      "name": "JSON punctuation (Tidelight Contrast)",
      "scope": [
        "punctuation.support.type.property-name.json",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.separator.array.json"
      ],
      "settings": { "foreground": "#e2e8f0" }
    },

    {
      "name": "Markdown headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": { "foreground": "#22d3ee", "fontStyle": "bold" }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": { "foreground": "#ffffff", "fontStyle": "bold" }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": { "foreground": "#ffffff", "fontStyle": "italic" }
    },
    {
      "name": "Markdown inline code",
      "scope": ["markup.inline.raw", "markup.inline.raw.string.markdown"],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "Markdown code blocks",
      "scope": ["markup.fenced_code.block", "markup.raw.block"],
      "settings": { "foreground": "#f8fafc" }
    },
    {
      "name": "Markdown links",
      "scope": ["markup.underline.link", "meta.link.inline", "meta.link.reference"],
      "settings": { "foreground": "#60a5fa", "fontStyle": "underline" }
    },

    {
      "name": "Diff inserted",
      "scope": ["markup.inserted", "diff.inserted", "meta.diff.header.to-file"],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "Diff deleted",
      "scope": ["markup.deleted", "diff.deleted", "meta.diff.header.from-file"],
      "settings": { "foreground": "#fb7185" }
    },
    {
      "name": "Diff changed",
      "scope": ["markup.changed", "diff.changed"],
      "settings": { "foreground": "#fbbf24" }
    },

    {
      "name": "TODO / FIXME / NOTE",
      "scope": [
        "comment.keyword.todo",
        "comment.keyword.fixme",
        "comment.keyword.note",
        "keyword.other.todo",
        "keyword.other.fixme",
        "keyword.other.note"
      ],
      "settings": { "foreground": "#fbbf24", "fontStyle": "bold" }
    },

    {
      "name": "Regex (common scopes)",
      "scope": [
        "string.regexp",
        "string.regexp keyword.other",
        "string.regexp punctuation.definition.string",
        "string.regexp punctuation.definition.group",
        "string.regexp punctuation.definition.character-class",
        "string.regexp constant.character.escape"
      ],
      "settings": { "foreground": "#22d3ee" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#93c5fd",
    "class": "#93c5fd",
    "interface": "#93c5fd",
    "type": "#93c5fd",
    "typeParameter": "#22d3ee",
    "parameter": "#ffffff",
    "property": "#c7d2fe",
    "variable": "#ffffff",
    "function": "#60a5fa",
    "method": "#60a5fa",
    "keyword": "#22d3ee",
    "string": "#a3e635",
    "number": "#fbbf24"
  }
}
{
  "name": "Tidelight Midnight",
  "type": "dark",
  "colors": {
    "focusBorder": "#22d3ee",

    "foreground": "#e2e8f0",
    "descriptionForeground": "#a1a1aa",
    "errorForeground": "#fb7185",

    "editor.background": "#070b14",
    "editor.foreground": "#e2e8f0",

    "editorLineNumber.foreground": "#1f2a3d",
    "editorLineNumber.activeForeground": "#a1a1aa",

    "editorCursor.foreground": "#22d3ee",
    "editor.selectionBackground": "#0891b233",
    "editor.inactiveSelectionBackground": "#0891b220",
    "editor.wordHighlightBackground": "#38bdf81f",
    "editor.wordHighlightStrongBackground": "#38bdf830",
    "editor.findMatchBackground": "#f59e0b40",
    "editor.findMatchHighlightBackground": "#f59e0b26",
    "editor.hoverHighlightBackground": "#22d3ee14",
    "editor.lineHighlightBackground": "#0b1220a6",

    "editorIndentGuide.background1": "#111827",
    "editorIndentGuide.activeBackground1": "#334155",

    "editorBracketMatch.background": "#22d3ee24",
    "editorBracketMatch.border": "#22d3ee80",

    "editorGroupHeader.tabsBackground": "#070b14",
    "tab.activeBackground": "#0b1220",
    "tab.activeBorderTop": "#22d3ee",
    "tab.inactiveBackground": "#070b14",
    "tab.activeForeground": "#f8fafc",
    "tab.inactiveForeground": "#a1a1aa",
    "tab.border": "#0b1220",

    "activityBar.background": "#060a12",
    "activityBar.foreground": "#e2e8f0",
    "activityBar.inactiveForeground": "#64748b",
    "activityBar.border": "#0b1220",

    "sideBar.background": "#060a12",
    "sideBar.foreground": "#e2e8f0",
    "sideBar.border": "#0b1220",
    "sideBarTitle.foreground": "#f8fafc",

    "list.activeSelectionBackground": "#0891b233",
    "list.activeSelectionForeground": "#f8fafc",
    "list.hoverBackground": "#0b1220",
    "list.hoverForeground": "#f8fafc",
    "list.inactiveSelectionBackground": "#0b1220",

    "statusBar.background": "#060a12",
    "statusBar.foreground": "#e2e8f0",
    "statusBar.border": "#0b1220",
    "statusBar.debuggingBackground": "#1d4ed8",
    "statusBar.debuggingForeground": "#f8fafc",

    "titleBar.activeBackground": "#060a12",
    "titleBar.activeForeground": "#e2e8f0",
    "titleBar.inactiveBackground": "#060a12",
    "titleBar.inactiveForeground": "#64748b",

    "panel.background": "#070b14",
    "panel.border": "#0b1220",

    "terminal.background": "#070b14",
    "terminal.foreground": "#e2e8f0",
    "terminalCursor.foreground": "#22d3ee",
    "terminal.ansiBlack": "#0b1220",
    "terminal.ansiBlue": "#38bdf8",
    "terminal.ansiCyan": "#22d3ee",
    "terminal.ansiGreen": "#a3e635",
    "terminal.ansiMagenta": "#f472b6",
    "terminal.ansiRed": "#fb7185",
    "terminal.ansiYellow": "#f59e0b",
    "terminal.ansiWhite": "#f8fafc",

    "input.background": "#0b1220",
    "input.foreground": "#f8fafc",
    "input.placeholderForeground": "#64748b",
    "input.border": "#111827",

    "dropdown.background": "#0b1220",
    "dropdown.foreground": "#f8fafc",
    "dropdown.border": "#111827",

    "badge.background": "#22d3ee",
    "badge.foreground": "#06242b",

    "notificationCenterHeader.background": "#060a12",
    "notifications.background": "#0b1220",
    "notifications.foreground": "#f8fafc",
    "notifications.border": "#111827",

    "editorOverviewRuler.errorForeground": "#fb7185cc",
    "editorOverviewRuler.warningForeground": "#f59e0bcc",
    "editorOverviewRuler.infoForeground": "#38bdf8cc",

    "gitDecoration.addedResourceForeground": "#a3e635",
    "gitDecoration.modifiedResourceForeground": "#38bdf8",
    "gitDecoration.deletedResourceForeground": "#fb7185",
    "gitDecoration.untrackedResourceForeground": "#22d3ee",
    "gitDecoration.ignoredResourceForeground": "#64748b"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#64748b", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "Template strings / interpolation",
      "scope": [
        "string.template",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "meta.template.expression"
      ],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#f59e0b" }
    },
    {
      "name": "Language constants (true/false/null/etc.)",
      "scope": ["constant.language"],
      "settings": { "foreground": "#f472b6" }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Operators / punctuation",
      "scope": ["keyword.operator", "punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#b6c2cf" }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "meta.method-call"
      ],
      "settings": { "foreground": "#38bdf8" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.parameters", "meta.function.parameters"],
      "settings": { "foreground": "#f8fafc" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable", "entity.name.variable"],
      "settings": { "foreground": "#f8fafc" }
    },
    {
      "name": "Constants",
      "scope": ["variable.other.constant", "entity.name.constant"],
      "settings": { "foreground": "#f59e0b" }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.struct",
        "entity.name.enum"
      ],
      "settings": { "foreground": "#93c5fd" }
    },
    {
      "name": "Type parameters / generics",
      "scope": ["entity.name.type.parameter", "meta.type.parameters", "meta.type.annotation"],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Language keywords for modules (import/export/etc.)",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Self references (this/self/super)",
      "scope": ["variable.language.this", "variable.language.super", "variable.language.self"],
      "settings": { "foreground": "#f472b6" }
    },
    {
      "name": "Property names",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": { "foreground": "#a5b4fc" }
    },
    {
      "name": "Tags (HTML/XML/JSX)",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#22d3ee" }
    },
    {
      "name": "Attributes (HTML/XML/JSX)",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#93c5fd" }
    },
    {
      "name": "Invalid / deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": { "foreground": "#fb7185", "fontStyle": "underline" }
    },

    {
      "name": "Markdown headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#22d3ee", "fontStyle": "bold" }
    },
    {
      "name": "Markdown bold/italic",
      "scope": ["markup.bold", "markup.italic"],
      "settings": { "foreground": "#e2e8f0" }
    },
    {
      "name": "Markdown inline code / fenced code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block", "markup.raw.block"],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "Markdown links",
      "scope": ["markup.underline.link", "string.other.link", "meta.link.inline"],
      "settings": { "foreground": "#38bdf8", "fontStyle": "underline" }
    },

    {
      "name": "Diff inserted",
      "scope": ["markup.inserted", "meta.diff.header.from-file", "punctuation.definition.inserted"],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "Diff deleted",
      "scope": ["markup.deleted", "meta.diff.header.to-file", "punctuation.definition.deleted"],
      "settings": { "foreground": "#fb7185" }
    },
    {
      "name": "Diff changed",
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": { "foreground": "#f59e0b" }
    },
    {
      "name": "Diff headers / ranges",
      "scope": ["meta.diff.header", "meta.diff.range", "meta.diff.index"],
      "settings": { "foreground": "#93c5fd" }
    },

    {
      "name": "TODO / FIXME / NOTE",
      "scope": [
        "comment.keyword.todo",
        "comment.keyword.fixme",
        "comment.keyword.note",
        "keyword.other.todo",
        "keyword.other.fixme",
        "keyword.other.note"
      ],
      "settings": { "foreground": "#f59e0b", "fontStyle": "bold" }
    },

    {
      "name": "JSON keys (Tidelight Midnight)",
      "scope": [
        "support.type.property-name.json",
        "meta.object-literal.key.json string.quoted.double.json",
        "meta.object.member.json string.quoted.double.json"
      ],
      "settings": { "foreground": "#a5b4fc" }
    },
    {
      "name": "JSON string values (Tidelight Midnight)",
      "scope": [
        "meta.object-literal.value.json string.quoted.double.json",
        "meta.array.literal.json string.quoted.double.json",
        "string.quoted.double.json"
      ],
      "settings": { "foreground": "#a3e635" }
    },
    {
      "name": "JSON numbers (Tidelight Midnight)",
      "scope": ["constant.numeric.json"],
      "settings": { "foreground": "#f59e0b" }
    },
    {
      "name": "JSON booleans & null (Tidelight Midnight)",
      "scope": [
        "constant.language.boolean.json",
        "constant.language.null.json",
        "constant.language.json"
      ],
      "settings": { "foreground": "#f472b6" }
    },
    {
      "name": "JSON punctuation (Tidelight Midnight)",
      "scope": [
        "punctuation.support.type.property-name.json",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.separator.array.json"
      ],
      "settings": { "foreground": "#b6c2cf" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#93c5fd",
    "class": "#93c5fd",
    "interface": "#93c5fd",
    "type": "#93c5fd",
    "typeParameter": "#22d3ee",
    "parameter": "#f8fafc",
    "property": "#a5b4fc",
    "variable": "#f8fafc",
    "function": "#38bdf8",
    "method": "#38bdf8",
    "keyword": "#22d3ee",
    "string": "#a3e635",
    "number": "#f59e0b"
  }
}
{
  "name": "Tidelight Dusk",
  "type": "dark",
  "colors": {
    "focusBorder": "#22d3ee",

    "foreground": "#cbd5e1",
    "descriptionForeground": "#94a3b8",
    "errorForeground": "#fb7185",

    "editor.background": "#0f172a",
    "editor.foreground": "#cbd5e1",
    "editorLineNumber.foreground": "#334155",
    "editorLineNumber.activeForeground": "#94a3b8",

    "editorCursor.foreground": "#22d3ee",
    "editor.selectionBackground": "#0e749033",
    "editor.inactiveSelectionBackground": "#0e749020",
    "editor.wordHighlightBackground": "#38bdf81a",
    "editor.wordHighlightStrongBackground": "#38bdf826",
    "editor.findMatchBackground": "#f59e0b33",
    "editor.findMatchHighlightBackground": "#f59e0b1f",
    "editor.hoverHighlightBackground": "#22d3ee12",
    "editor.lineHighlightBackground": "#111c3380",

    "editorIndentGuide.background1": "#1e293b",
    "editorIndentGuide.activeBackground1": "#334155",

    "editorBracketMatch.background": "#22d3ee1f",
    "editorBracketMatch.border": "#22d3ee66",

    "editorGroupHeader.tabsBackground": "#0f172a",
    "tab.activeBackground": "#111c33",
    "tab.inactiveBackground": "#0f172a",
    "tab.activeForeground": "#e2e8f0",
    "tab.inactiveForeground": "#94a3b8",
    "tab.border": "#0f172a",

    "activityBar.background": "#0d162b",
    "activityBar.foreground": "#cbd5e1",
    "activityBar.inactiveForeground": "#64748b",
    "activityBar.border": "#111c33",

    "sideBar.background": "#0d162b",
    "sideBar.foreground": "#cbd5e1",
    "sideBar.border": "#111c33",
    "sideBarTitle.foreground": "#e2e8f0",

    "list.activeSelectionBackground": "#0e749033",
    "list.activeSelectionForeground": "#e2e8f0",
    "list.hoverBackground": "#111c33",
    "list.hoverForeground": "#e2e8f0",
    "list.inactiveSelectionBackground": "#111c33",

    "statusBar.background": "#0d162b",
    "statusBar.foreground": "#cbd5e1",
    "statusBar.border": "#111c33",
    "statusBar.debuggingBackground": "#1d4ed8",
    "statusBar.debuggingForeground": "#f8fafc",

    "titleBar.activeBackground": "#0d162b",
    "titleBar.activeForeground": "#cbd5e1",
    "titleBar.inactiveBackground": "#0d162b",
    "titleBar.inactiveForeground": "#64748b",

    "panel.background": "#0f172a",
    "panel.border": "#111c33",

    "terminal.background": "#0f172a",
    "terminal.foreground": "#cbd5e1",
    "terminalCursor.foreground": "#22d3ee",
    "terminal.ansiBlack": "#0f172a",
    "terminal.ansiBlue": "#38bdf8",
    "terminal.ansiCyan": "#22d3ee",
    "terminal.ansiGreen": "#a3e635",
    "terminal.ansiMagenta": "#f472b6",
    "terminal.ansiRed": "#fb7185",
    "terminal.ansiYellow": "#f59e0b",
    "terminal.ansiWhite": "#e2e8f0",

    "input.background": "#111c33",
    "input.foreground": "#e2e8f0",
    "input.placeholderForeground": "#64748b",
    "input.border": "#1e293b",

    "dropdown.background": "#111c33",
    "dropdown.foreground": "#e2e8f0",
    "dropdown.border": "#1e293b",

    "badge.background": "#22d3ee",
    "badge.foreground": "#06242b",

    "notificationCenterHeader.background": "#0d162b",
    "notifications.background": "#111c33",
    "notifications.foreground": "#e2e8f0",
    "notifications.border": "#1e293b",

    "editorOverviewRuler.errorForeground": "#fb7185cc",
    "editorOverviewRuler.warningForeground": "#f59e0bcc",
    "editorOverviewRuler.infoForeground": "#38bdf8cc",

    "gitDecoration.addedResourceForeground": "#a3e635",
    "gitDecoration.modifiedResourceForeground": "#38bdf8",
    "gitDecoration.deletedResourceForeground": "#fb7185",
    "gitDecoration.untrackedResourceForeground": "#22d3ee",
    "gitDecoration.ignoredResourceForeground": "#64748b"
  },
  "tokenColors": [
    { "name": "Comments", "scope": ["comment", "punctuation.definition.comment"], "settings": { "foreground": "#64748b", "fontStyle": "italic" } },
    { "name": "Documentation comments", "scope": ["comment.block.documentation", "comment.line.documentation", "comment.documentation"], "settings": { "foreground": "#7c8aa3", "fontStyle": "italic" } },

    { "name": "Strings", "scope": ["string", "punctuation.definition.string"], "settings": { "foreground": "#a3e635" } },
    { "name": "Template strings / interpolation", "scope": ["string.template", "punctuation.definition.template-expression", "punctuation.section.embedded", "meta.template.expression"], "settings": { "foreground": "#22d3ee" } },

    { "name": "Numbers", "scope": ["constant.numeric"], "settings": { "foreground": "#f59e0b" } },
    { "name": "Language constants", "scope": ["constant.language"], "settings": { "foreground": "#f472b6" } },

    { "name": "Keywords", "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control", "keyword.operator.new", "keyword.operator.expression", "keyword.operator.logical"], "settings": { "foreground": "#22d3ee" } },
    { "name": "Operators / punctuation", "scope": ["keyword.operator", "punctuation", "meta.brace", "meta.delimiter"], "settings": { "foreground": "#94a3b8" } },

    { "name": "Functions", "scope": ["entity.name.function", "support.function", "meta.function-call", "meta.method-call"], "settings": { "foreground": "#38bdf8" } },
    { "name": "Parameters", "scope": ["variable.parameter", "meta.parameters", "meta.function.parameters"], "settings": { "foreground": "#e2e8f0" } },
    { "name": "Variables", "scope": ["variable", "meta.definition.variable", "entity.name.variable"], "settings": { "foreground": "#e2e8f0" } },
    { "name": "Constants", "scope": ["variable.other.constant", "entity.name.constant"], "settings": { "foreground": "#f59e0b" } },

    { "name": "Types", "scope": ["entity.name.type", "support.type", "support.class", "entity.name.class", "entity.name.interface", "entity.name.struct", "entity.name.enum"], "settings": { "foreground": "#93c5fd" } },
    { "name": "Type parameters / annotations", "scope": ["entity.name.type.parameter", "meta.type.parameters", "meta.type.annotation"], "settings": { "foreground": "#22d3ee" } },

    { "name": "Self references (this/self/super)", "scope": ["variable.language.this", "variable.language.super", "variable.language.self"], "settings": { "foreground": "#f472b6" } },
    { "name": "Property names", "scope": ["variable.other.property", "support.variable.property"], "settings": { "foreground": "#a5b4fc" } },

    { "name": "Tags (HTML/XML/JSX)", "scope": ["entity.name.tag"], "settings": { "foreground": "#22d3ee" } },
    { "name": "Attributes (HTML/XML/JSX)", "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#93c5fd" } },

    { "name": "Invalid / deprecated", "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#fb7185", "fontStyle": "underline" } },


    {
      "name": "JSON keys",
      "scope": ["support.type.property-name.json", "meta.object-literal.key.json string.quoted.double.json", "meta.object.member.json string.quoted.double.json"],
      "settings": { "foreground": "#a5b4fc" }
    },
    {
      "name": "JSON string values",
      "scope": ["meta.object-literal.value.json string.quoted.double.json", "meta.array.literal.json string.quoted.double.json", "string.quoted.double.json"],
      "settings": { "foreground": "#a3e635" }
    },
    { "name": "JSON numbers", "scope": ["constant.numeric.json"], "settings": { "foreground": "#f59e0b" } },
    { "name": "JSON booleans & null", "scope": ["constant.language.boolean.json", "constant.language.null.json", "constant.language.json"], "settings": { "foreground": "#f472b6" } },
    {
      "name": "JSON punctuation",
      "scope": ["punctuation.support.type.property-name.json", "punctuation.separator.dictionary.key-value.json", "punctuation.definition.dictionary.begin.json", "punctuation.definition.dictionary.end.json", "punctuation.definition.array.begin.json", "punctuation.definition.array.end.json", "punctuation.separator.array.json"],
      "settings": { "foreground": "#94a3b8" }
    },

    { "name": "Markdown headings", "scope": ["markup.heading", "markup.heading entity.name", "entity.name.section"], "settings": { "foreground": "#22d3ee", "fontStyle": "bold" } },
    { "name": "Markdown bold", "scope": ["markup.bold", "punctuation.definition.bold"], "settings": { "fontStyle": "bold" } },
    { "name": "Markdown italic", "scope": ["markup.italic", "punctuation.definition.italic"], "settings": { "fontStyle": "italic" } },
    { "name": "Markdown inline code", "scope": ["markup.inline.raw", "markup.fenced_code.block", "markup.raw.inline"], "settings": { "foreground": "#a3e635" } },
    { "name": "Markdown links", "scope": ["markup.underline.link", "meta.link.inline", "string.other.link", "constant.other.reference.link"], "settings": { "foreground": "#38bdf8", "fontStyle": "underline" } },

    { "name": "Diff headers", "scope": ["meta.diff", "meta.diff.header", "meta.separator.diff"], "settings": { "foreground": "#94a3b8" } },
    { "name": "Diff inserted", "scope": ["markup.inserted", "meta.diff.range.inserted", "meta.diff.header.to-file"], "settings": { "foreground": "#a3e635" } },
    { "name": "Diff deleted", "scope": ["markup.deleted", "meta.diff.range.deleted", "meta.diff.header.from-file"], "settings": { "foreground": "#fb7185" } },
    { "name": "Diff changed", "scope": ["markup.changed", "meta.diff.range.changed"], "settings": { "foreground": "#f59e0b" } },

    { "name": "TODO/FIXME/NOTE in comments", "scope": ["comment.keyword.todo", "comment.keyword.fixme", "keyword.other.todo", "keyword.other.fixme", "keyword.other.note"], "settings": { "foreground": "#f59e0b", "fontStyle": "bold" } },

    {
      "name": "Regex (general)",
      "scope": ["string.regexp", "string.regexp keyword.operator", "string.regexp punctuation.definition", "constant.character.escape", "constant.other.character-class", "constant.other.character-class.regexp"],
      "settings": { "foreground": "#f472b6" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#93c5fd",
    "class": "#93c5fd",
    "interface": "#93c5fd",
    "type": "#93c5fd",
    "typeParameter": "#22d3ee",
    "parameter": "#e2e8f0",
    "property": "#a5b4fc",
    "variable": "#e2e8f0",
    "function": "#38bdf8",
    "method": "#38bdf8",
    "keyword": "#22d3ee",
    "string": "#a3e635",
    "number": "#f59e0b"
  }
}